		"eslint": "^7.32.0",
		"eslint-plugin-svelte3": "^3.4.1",
		"feather-icons": "^4.29.0",
		"gh-pages": "^3.2.3",
		"svelte": "^3.48.0",
		"svelte-check": "^2.8.0",
		"svelte-i18n": "^3.4.0",
		"svelte-preprocess": "^4.10.7",
		"tslib": "^2.4.0",
		"typescript": "^4.7.4"
	},
	"type": "module"
